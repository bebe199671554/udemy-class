{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "udemy-class",
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "udemy-class": {
      "projectType": "application",
      "root": "apps/udemy-class",
      "sourceRoot": "apps/udemy-class/src",
      "prefix": "udemy-class",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/udemy-class",
            "index": "apps/udemy-class/src/index.html",
            "main": "apps/udemy-class/src/main.ts",
            "polyfills": "apps/udemy-class/src/polyfills.ts",
            "tsConfig": "apps/udemy-class/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/udemy-class/src/favicon.ico",
              "apps/udemy-class/src/assets"
            ],
            "styles": ["apps/udemy-class/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/udemy-class/src/environments/environment.ts",
                  "with": "apps/udemy-class/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "udemy-class:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "udemy-class:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "udemy-class:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/udemy-class/tsconfig.app.json",
              "apps/udemy-class/tsconfig.spec.json",
              "apps/udemy-class/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/udemy-class/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/udemy-class"],
          "options": {
            "jestConfig": "apps/udemy-class/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "udemy-class-e2e": {
      "root": "apps/udemy-class-e2e",
      "sourceRoot": "apps/udemy-class-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/udemy-class-e2e/cypress.json",
            "tsConfig": "apps/udemy-class-e2e/tsconfig.e2e.json",
            "devServerTarget": "udemy-class:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "udemy-class:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/udemy-class-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/udemy-class-e2e/**/*"]
          }
        }
      }
    }
  }
}
